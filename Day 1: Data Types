#include <iostream>
#include <iomanip>
#include <limits>

using namespace std;

int main() {
    int i = 4;
    double d = 4.0;
    string s = "HackerRank ";
    // Declare variables for input
    int i2;
    double d2;
    string s2;

    // Read input
    cin >> i2;
    cin >> d2;
    cin.ignore(); // Ignore the newline character
    getline(cin, s2);

    // Perform operations
    int sum_int = i + i2;
    double sum_double = d + d2;

    // Print output
    cout << sum_int << endl;
    cout << fixed << setprecision(1) << sum_double << endl;
    cout << s + s2 << endl;



    return 0;
}
